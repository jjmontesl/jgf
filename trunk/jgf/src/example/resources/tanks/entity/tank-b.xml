<?xml version="1.0" encoding="ISO-8859-1" ?>

<!--
	Java Game Framework 
	Scene Loader File
	$Id: tank-a.xml 97 2008-12-02 01:52:28Z jjmontes $
 --> 	

<jgf>

	<loader>
	
		<loader id="!entity.creator" class="net.jgf.loader.entity.EntityCreatorLoader" >
			<property name="EntityCreatorLoader.entityClass" value="net.jgf.example.tanks.entity.AITank" />
		</loader>
		
		<loader id="!entity.attributes" class="net.jgf.loader.AttributeLoader" >
			<property name="AttributeLoader.maxSpeed" value="1.9" />
			<property name="AttributeLoader.firePerSecond" value="0.9" />
			<property name="AttributeLoader.simultaneousBullets" value="5" />
		</loader>

		<loader id="!entity.model" class="net.jgf.jme.loader.entity.EntityModelLoader" >
			<property name="ConverterLoader.resourceUrl" value="tanks/model/enemy-yellow/yellowtank.dae" />
		</loader>
		
	</loader>
	
	
</jgf>
 


