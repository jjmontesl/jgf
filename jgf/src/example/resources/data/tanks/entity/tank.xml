<?xml version="1.0" encoding="ISO-8859-1" ?>

<!--
	Java Game Framework 
	Scene Loader File
	$Id$
 --> 	

<jgf>

	<loader>
	
		<loader id="!entity.creator" class="net.jgf.loader.entity.EntityCreatorLoader" >
			<property name="EntityCreatorLoader.entityClass" value="net.jgf.example.tanks.entity.PlayerTank" />
		</loader>

		<loader id="!entity.model" class="net.jgf.jme.loader.entity.EntityModelLoader" >
			<property name="ConverterLoader.resourceUrl" value="tanks/model/tank/tank.dae" />
		</loader>

        <loader id="!entity.attributes" class="net.jgf.loader.AttributeLoader" >
            <property name="AttributeLoader.simultaneousBullets" value="5" />
        </loader>
		
	</loader>
	
	
</jgf>
 


